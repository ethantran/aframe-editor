{
  "name": "aframe-editor",
  "version": "0.0.1",
  "description": "demo example using aframe react and redux",
  "dependencies": {
    "aframe": "^0.2.0",
    "aframe-react": "^1.2.0",
    "babel-polyfill": "^6.3.14",
    "lodash": "^4.10.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-form": "^5.0.1",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.9.5"
  },
  "scripts": {
    "build": "webpack",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages",
    "serve": "cd src && python -m SimpleHTTPServer 5555",
    "serve-js": "webpack-dev-server --content-base src --progress --watch --history-api-fallback --inline --hot"
  },
  "homepage": "https://github.com/ethantran/aframe-editor",
  "bugs": "https://github.com/ethantran/aframe-editor/issues",
